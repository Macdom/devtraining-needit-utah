        <script><![CDATA[var NeedItUtils = Class.create();
NeedItUtils.prototype = {
    initialize: function() {
    },
	isDatePast: function(strDate) {
		// Create GlideDateTime objects for the current date and the passed in date
		var rightnow = new GlideDateTime().getLocalDate();
		var testdate = new GlideDateTime(strDate).getDate();

		// If the testdate is on or before rightnow, return true
		return (testdate.onOrBefore(rightnow));
	},
    type: 'NeedItUtils'
};]]></script>
